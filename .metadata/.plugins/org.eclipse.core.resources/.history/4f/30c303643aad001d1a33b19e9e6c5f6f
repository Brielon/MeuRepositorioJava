package classe;

public class ProdutoTeste {
	public static void main(String[] args) {
		produto n1 = new produto();
		n1.nome = "Carro";
		n1.preco = 9_000.00;
		n1.desconto = 0.3;
		System.out.print("%s\nTem o preco de R$%.2f.\nporen com o desconto o valor final fica em R$%.2f"+n1.nome+n1.preco+n1.precoF);
	}
}
